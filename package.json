{
  "name": "svelte-baked-cookie",
  "description": "üç™ Universal accessible hard-baked cookies for SvelteKit",
  "version": "0.0.4",
  "main": "dist/index.js",
  "type": "module",
  "license": "MIT",
  "author": {
    "name": "jill64",
    "email": "143883742+jill64@users.noreply.github.com",
    "url": "https://github.com/jill64",
    "image": "https://avatars.githubusercontent.com/u/143883742?v=4"
  },
  "bugs": "https://github.com/jill64/svelte-baked-cookie/issues",
  "homepage": "https://github.com/jill64/svelte-baked-cookie#readme",
  "files": [
    "dist",
    "!**/*.test.*",
    "!**/*.spec.*",
    "!*.test.*",
    "!*.spec.*"
  ],
  "exports": {
    "./serde": {
      "types": "./dist/serde.d.ts",
      "default": "./dist/serde.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jill64/svelte-baked-cookie.git",
    "image": "https://opengraph.githubassets.com/90d742ffc4220b7de94ffa4e7788782012bed39f30a64dc16766dee84d6db9d4/jill64/svelte-baked-cookie"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "cookie",
    "sveltekit",
    "type-safe"
  ],
  "prettier": "@jill64/prettier-config",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "npm run build && vite preview",
    "prepack": "npm run build",
    "package": "svelte-kit sync && npx @sveltejs/package && npx publint",
    "check": "svelte-kit sync && npx svelte-check",
    "lint": "npm run check && npx eslint .",
    "format": "npx @jill64/psx",
    "test": "playwright test"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^1.0.0",
    "svelte": "^4.0.0"
  },
  "devDependencies": {
    "@jill64/eslint-config-svelte": "1.0.2",
    "@jill64/playwright-config": "2.2.0",
    "@jill64/prettier-config": "1.0.0",
    "@jill64/sentry-sveltekit-cloudflare": "1.5.0",
    "@jill64/svelte-dark-theme": "2.2.14",
    "@jill64/svelte-tailwind": "1.0.0",
    "@playwright/test": "1.40.1",
    "@sveltejs/adapter-cloudflare": "2.3.4",
    "svelte-highlight-switcher": "1.0.0",
    "@sveltejs/kit": "1.28.0",
    "svelte": "4.2.8",
    "svelte-highlight": "7.4.2",
    "typescript": "5.3.3",
    "vite": "5.0.7",
    "vitest": "1.0.4"
  },
  "dependencies": {
    "@jill64/transform": "1.0.0",
    "@types/cookie": "0.6.0",
    "cookie": "0.6.0",
    "ts-serde": "1.0.0"
  }
}
