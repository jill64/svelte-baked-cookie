  "version": "1.0.31",
  "main": "dist/index.js",
  "type": "module",
  "license": "MIT",
  "author": {
    "name": "jill64",
    "email": "contact@jill64.dev",
    "url": "https://github.com/jill64",
    "image": "https://avatars.githubusercontent.com/u/143883742?v=4"
  },
  "bugs": "https://github.com/jill64/svelte-baked-cookie/issues",
  "homepage": "https://github.com/jill64/svelte-baked-cookie#readme",
  "files": [
    "dist"
  ],
  "exports": {
    "./serde": {
      "types": "./dist/serde.d.ts",
      "default": "./dist/serde.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jill64/svelte-baked-cookie.git",
    "image": "https://opengraph.githubassets.com/7972f0f3765ceac1bcf03c106c115e189823dfa93aae8d0a8c321a6a9ab3aa74/jill64/svelte-baked-cookie"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "cookie",
    "isomophism",
    "sveltekit",
    "type-safe"
  ],
  "prettier": "@jill64/prettier-config",
  "scripts": {
    "dev": "npm run package && vite dev",
    "build": "npm run package && vite build",
    "preview": "npm run build && vite preview",
    "prepack": "npm run build",
    "package": "svelte-kit sync && npx @sveltejs/package && npx publint",
    "check": "svelte-kit sync && npx svelte-check",
    "lint": "npm run package && npm run check && npx eslint .",
    "format": "npx @jill64/psx",
    "test": "playwright test"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^2.0.0",
    "svelte": "^4.0.0"
