{
  "name": "svelte-baked-cookie",
  "description": "üç™ Universal accessible hard-baked cookies for SvelteKit",
  "version": "0.0.1",
  "main": "dist/index.js",
  "type": "module",
  "license": "MIT",
  "author": "jill64 <intents.turrets0h@icloud.com> (https://github.com/jill64)",
  "bugs": "https://github.com/jill64/svelte-baked-cookie/issues",
  "homepage": "https://github.com/jill64/svelte-baked-cookie#readme",
  "files": [
    "dist",
    "!**/*.test.*",
    "!**/*.spec.*",
    "!*.test.*",
    "!*.spec.*"
  ],
  "exports": {
    "./serde": {
      "types": "./dist/serde.d.ts",
      "default": "./dist/serde.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jill64/svelte-baked-cookie.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "cookie",
    "sveltekit",
    "type-safe"
  ],
  "prettier": "@jill64/prettier-config",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "npm run build && vite preview",
    "prepack": "npm run build",
    "package": "svelte-kit sync && npx @sveltejs/package && npx publint",
    "check": "svelte-kit sync && npx svelte-check",
    "lint": "npm run check && npx eslint .",
    "format": "npx @jill64/psx",
    "test": "playwright test"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^1.0.0",
    "svelte": "^4.0.0"
  },
  "devDependencies": {
    "@jill64/eslint-config-svelte": "0.0.7",
    "@jill64/playwright-config": "0.0.1",
    "@jill64/prettier-config": "0.0.2",
    "@jill64/sentry-sveltekit-cloudflare": "1.4.2",
    "@jill64/svelte-dark-theme": "2.2.6",
    "@jill64/svelte-tailwind": "0.0.6",
    "@playwright/test": "1.40.1",
    "@sveltejs/adapter-cloudflare": "2.3.3",
    "svelte-highlight-switcher": "0.0.1",
    "@sveltejs/kit": "1.27.6",
    "svelte": "4.2.8",
    "svelte-highlight": "7.4.2",
    "typescript": "5.3.2",
    "vite": "5.0.4",
    "vitest": "0.34.6"
  },
  "dependencies": {
    "@jill64/transform": "0.0.1",
    "@types/cookie": "0.6.0",
    "cookie": "0.6.0",
    "ts-serde": "0.4.0"
  }
}