  "version": "1.0.23",
  "main": "dist/index.js",
  "type": "module",
  "license": "MIT",
  "author": {
    "name": "jill64",
    "email": "contact@jill64.dev",
    "url": "https://github.com/jill64",
    "image": "https://avatars.githubusercontent.com/u/143883742?v=4"
  },
  "bugs": "https://github.com/jill64/svelte-baked-cookie/issues",
  "homepage": "https://github.com/jill64/svelte-baked-cookie#readme",
  "files": [
    "dist"
  ],
  "exports": {
    "./serde": {
      "types": "./dist/serde.d.ts",
      "default": "./dist/serde.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jill64/svelte-baked-cookie.git",
    "image": "https://opengraph.githubassets.com/720db2a3ffb720b50b13413e0949eb92d290affcc5d8a7b172aebbf307de6b59/jill64/svelte-baked-cookie"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "cookie",
    "isomophism",
    "sveltekit",
    "type-safe"
  ],
  "prettier": "@jill64/prettier-config",
  "scripts": {
    "dev": "npm run package && vite dev",
    "build": "npm run package && vite build",
    "preview": "npm run build && vite preview",
    "prepack": "npm run build",
    "package": "svelte-kit sync && npx @sveltejs/package && npx publint",
    "check": "svelte-kit sync && npx svelte-check",
    "lint": "npm run package && npm run check && npx eslint .",
    "format": "npx @jill64/psx",
    "test": "playwright test"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^2.0.0",
    "svelte": "^4.0.0"
  },
  "devDependencies": {
    "@jill64/eslint-config-svelte": "1.2.22",
    "@jill64/npm-demo-layout": "1.0.176",
    "@jill64/playwright-config": "2.2.5",
    "@jill64/prettier-config": "1.0.0",
    "@jill64/sentry-sveltekit-cloudflare": "1.7.11",
    "@jill64/svelte-input": "1.6.26",
    "@jill64/svelte-tailwind": "1.2.26",
    "@jill64/universal-sanitizer": "1.2.8",
    "@playwright/test": "1.43.1",
    "@sveltejs/adapter-cloudflare": "4.4.0",
    "@sveltejs/kit": "2.5.7",
    "@sveltejs/vite-plugin-svelte": "3.1.0",
    "svelte": "4.2.15",
    "typescript": "5.4.5",
    "vite": "5.2.10",
    "vitest": "1.5.0"
  },
  "dependencies": {
    "@jill64/transform": "1.0.2",
    "@types/cookie": "0.6.0",
    "cookie": "0.6.0",
    "ts-serde": "1.0.6"
  }
}
