{
  "name": "svelte-baked-cookie",
  "description": "üç™ Universal accessible hard-baked cookies for SvelteKit",
  "version": "1.1.2",
  "main": "dist/index.js",
  "type": "module",
  "license": "MIT",
  "author": {
    "name": "jill64",
    "email": "contact@jill64.dev",
    "url": "https://github.com/jill64",
    "image": "https://avatars.githubusercontent.com/u/143883742?v=4"
  },
  "bugs": "https://github.com/jill64/svelte-baked-cookie/issues",
  "homepage": "https://github.com/jill64/svelte-baked-cookie#readme",
  "files": [
    "dist"
  ],
  "exports": {
    "./serde": {
      "types": "./dist/serde.d.ts",
      "default": "./dist/serde.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jill64/svelte-baked-cookie.git",
    "image": "https://opengraph.githubassets.com/6bef7a7592b6295ac6cc56e2e11cfef67936e31aaa1ea35057b0c20a9c0e76c0/jill64/svelte-baked-cookie"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "cookie",
    "isomophism",
    "sveltekit",
    "type-safe"
  ],
  "prettier": "@jill64/prettier-config",
  "scripts": {
    "dev": "npm run package && vite dev",
    "build": "npm run package && vite build",
    "preview": "npm run build && vite preview",
    "prepack": "npm run build",
    "package": "svelte-kit sync && npx @sveltejs/package && npx publint",
    "check": "svelte-kit sync && npx svelte-check",
    "lint": "npm run package && npm run check && npx eslint .",
    "format": "npx @jill64/psx",
    "test": "playwright test"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^2.0.0",
    "svelte": "^4.0.0 || ^5.0.0"
  },
  "devDependencies": {
    "@jill64/eslint-config-svelte": "2.0.1",
    "@jill64/npm-demo-layout": "1.0.240",
    "@jill64/playwright-config": "2.4.2",
    "@jill64/prettier-config": "1.0.0",
    "@jill64/sentry-sveltekit-cloudflare": "2.0.1",
    "@jill64/svelte-input": "1.6.37",
    "@jill64/svelte-tailwind": "1.2.69",
    "@jill64/universal-sanitizer": "1.3.5",
    "@playwright/test": "1.49.1",
    "@sveltejs/adapter-cloudflare": "4.9.0",
    "@sveltejs/kit": "2.8.3",
    "@sveltejs/vite-plugin-svelte": "5.0.3",
    "svelte": "5.16.0",
    "typescript": "5.7.2",
    "vite": "6.0.6",
    "vitest": "2.1.8"
  },
  "dependencies": {
    "@jill64/transform": "1.0.4",
    "@types/cookie": "0.6.0",
    "cookie": "0.7.2",
    "ts-serde": "1.0.8"
  }
}
