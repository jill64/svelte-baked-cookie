{
  "name": "svelte-baked-cookie",
  "description": "üç™ Universal accessible hard-baked cookies for SvelteKit",
  "version": "2.0.3",
  "main": "dist/index.js",
  "type": "module",
  "license": "MIT",
  "author": {
    "name": "jill64",
    "email": "contact@jill64.dev",
    "url": "https://github.com/jill64",
    "image": "https://avatars.githubusercontent.com/u/143883742?v=4"
  },
  "bugs": "https://github.com/jill64/svelte-baked-cookie/issues",
  "homepage": "https://github.com/jill64/svelte-baked-cookie#readme",
  "files": [
    "dist"
  ],
  "exports": {
    "./serde": {
      "types": "./dist/serde.d.ts",
      "default": "./dist/serde.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jill64/svelte-baked-cookie.git",
    "image": "https://opengraph.githubassets.com/e65eda9cdb95dfc1f36d7b87b197b9c390f7e63a0b3b2001282c85aab749223e/jill64/svelte-baked-cookie"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "cookie",
    "isomophism",
    "library",
    "sveltekit",
    "type-safe"
  ],
  "prettier": "@jill64/prettier-config",
  "scripts": {
    "dev": "bun run package && vite dev",
    "build": "bun run package && vite build",
    "preview": "bun run build && vite preview",
    "prepack": "bun run build",
    "package": "svelte-kit sync && bunx @sveltejs/package && bunx publint",
    "check": "svelte-kit sync && bunx svelte-check",
    "lint": "bun run package && bun run check && bunx eslint .",
    "format": "bunx psvx",
    "test:e2e": "playwright test"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^2.0.0",
    "svelte": "^4.0.0 || ^5.0.0"
  },
  "devDependencies": {
    "@jill64/eslint-config-svelte": "2.2.1",
    "@jill64/npm-demo-layout": "2.0.17",
    "@jill64/playwright-config": "3.0.0",
    "@jill64/prettier-config": "1.0.0",
    "@jill64/sentry-sveltekit-cloudflare": "2.0.6",
    "@jill64/svelte-input": "2.0.3",
    "@jill64/svelte-tailwind": "1.2.80",
    "@playwright/test": "1.51.0",
    "@sveltejs/adapter-cloudflare": "5.0.3",
    "@sveltejs/kit": "2.19.1",
    "@sveltejs/vite-plugin-svelte": "5.0.3",
    "svelte": "5.23.0",
    "typescript": "5.8.2",
    "vite": "6.2.2"
  },
  "dependencies": {
    "@jill64/transform": "1.0.4",
    "@types/cookie": "1.0.0",
    "cookie": "1.0.2",
    "ts-serde": "1.0.9"
  }
}
