  "devDependencies": {
    "@jill64/eslint-config-svelte": "1.3.9",
    "@jill64/npm-demo-layout": "1.0.240",
    "@jill64/playwright-config": "2.4.1",
    "@jill64/prettier-config": "1.0.0",
    "@jill64/sentry-sveltekit-cloudflare": "1.7.16",
    "@jill64/svelte-input": "1.6.34",
    "@jill64/svelte-tailwind": "1.2.55",
    "@jill64/universal-sanitizer": "1.3.1",
    "@playwright/test": "1.46.1",
    "@sveltejs/adapter-cloudflare": "4.7.2",
    "@sveltejs/kit": "2.5.24",
    "@sveltejs/vite-plugin-svelte": "3.1.1",
    "svelte": "4.2.18",
    "typescript": "5.5.4",
    "vite": "5.4.1",
    "vitest": "2.0.5"
  },
  "dependencies": {
    "@jill64/transform": "1.0.3",
    "@types/cookie": "0.6.0",
    "cookie": "0.6.0",
    "ts-serde": "1.0.7"
  }
}
